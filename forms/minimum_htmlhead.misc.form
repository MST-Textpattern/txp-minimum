<txp:if_plugin name="aks_header" version="0.3">
   <txp:aks_header strip="1" gzip="1" />
</txp:if_plugin>
<!doctype html>
<html lang="en-us">
<head>
<meta charset="utf-8">
<title>
  <txp:if_status>
     <txp:if_article_list>
         <txp:if_category>
			<txp:category title="1" /> <txp:chh_if_data><txp:page_url type="pg" /> </txp:chh_if_data>&bull; <txp:site_name />
         <txp:else />
			<txp:if_section name=",default">
			    <txp:site_name />
            <txp:else />
                 <txp:section title="1" /> <txp:chh_if_data><txp:page_url type="pg" /> </txp:chh_if_data> &bull; <txp:site_name />
            </txp:if_section>
        </txp:if_category>
    </txp:if_article_list>
    <txp:if_individual_article>
    	<txp:title /> &bull; <txp:site_name />
    </txp:if_individual_article>
	<txp:else />
<txp:site_name />: <txp:error_status />
</txp:if_status>
</title>


   <meta name='robots' content='index' /> 
   <link rel='index' title='<txp:site_name />' href='<txp:site_url />' /> 
   <meta name="generator" content="Textpattern" /> 

   <txp:if_individual_article>
      <txp:if_keywords>
         <meta name="keywords" content="<txp:keywords />" />
      <txp:else />
         <meta name="keywords" content="<txp:variable name="meta_keywords" />" />
      </txp:if_keywords>
      <meta name="description" content="<txp:etz_striptags><txp:rss_auto_excerpt showlinkwithexcerpt="0" /></txp:etz_striptags>">
   </txp:if_individual_article>
   
   <txp:feed_link flavor="atom" format="link" label="Atom" section="" />
   <txp:feed_link flavor="rss" format="link" label="RSS" section="" />
   
<link rel="stylesheet" type="text/css" media="screen" href="<txp:css name="bootstrap_min" />" />  
   <link rel="stylesheet" type="text/css" media="screen" href="<txp:css name="minimum_style" />" />

   <link href='http://fonts.googleapis.com/css?family=Average|Donegal+One|Ovo' rel='stylesheet' type='text/css'>
</head>